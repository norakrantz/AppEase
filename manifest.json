{
    "name": "AppEase",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": "icon1.png",
        "default_title": "AppEase",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": "content.js"
        }
    ]
}